{
  "name": "otplib",
  "description": "HMAC-based (HOTP) and Time-based (TOTP) One-Time Password library",
  "version": "12.0.1",
  "main": "src/index.ts",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build:lib": "rollup -c ../../configs/rollup.config.js",
    "build:typedef": "tsc --emitDeclarationOnly -p tsconfig.build.json"
  },
  "dependencies": {
    "@otplib/core": "^12.0.1",
    "@otplib/preset-default": "^12.0.1",
    "@otplib/preset-v11": "^12.0.1"
  },
  "otplib": {
    "files": [
      "src/core.ts",
      "src/index.ts",
      "src/v11.ts"
    ]
  },
  "keywords": [
    "totp",
    "hotp",
    "otp",
    "authenticator",
    "one time password",
    "google authenticator",
    "authentication",
    "2FA",
    "2 factor",
    "node",
    "browser"
  ]
}
