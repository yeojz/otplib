{
  "name": "otplib-smoke-test",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Smoke tests for published otplib packages",
  "scripts": {
    "test": "node --test test/*.test.js && node --test --experimental-strip-types test/*.test.ts",
    "test:typecheck": "tsc --noEmit test/*.test.ts",
    "pretest": "bash install-packages.sh",
    "install:packages": "bash install-packages.sh"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
